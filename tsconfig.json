{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2015",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@src/*": ["./src/*"],
      "@type/*": ["./src/type/*"]
    }
  }
}
